<launch>

<rosparam file="$(find iiwa7_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="iiwa7" args="Rev44_position_controller Rev45_position_controller Rev46_position_controller Rev47_position_controller Rev48_position_controller Rev49_position_controller Rev50_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/iiwa7/joint_states"/>
</node>

</launch>